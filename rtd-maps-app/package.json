{
  "name": "rtd-maps-app",
  "version": "1.0.0",
  "private": true,
  "description": "Real-time RTD Denver transit visualization using Google Maps and live GTFS-RT data",
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.1.35",
    "@types/googlemaps": "^3.43.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "web-vitals": "^3.5.0",
    "kafkajs": "^2.2.4",
    "axios": "^1.6.2",
    "date-fns": "^3.0.6",
    "lucide-react": "^0.294.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.10",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "vite --port 3000",
    "type-check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "RTD",
    "Denver",
    "Transit",
    "GTFS-RT",
    "Real-time",
    "Maps",
    "React",
    "TypeScript",
    "Google Maps"
  ],
  "author": "RTD GTFS Pipeline Team",
  "license": "MIT"
}