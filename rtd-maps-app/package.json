{
  "name": "transitstream-maps",
  "version": "1.0.0",
  "private": true,
  "description": "TransitStream - Real-time RTD Denver transit visualization using OpenStreetMap and live GTFS-RT data",
  "dependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.2",
    "date-fns": "^3.0.6",
    "kafkajs": "^2.2.4",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.8.1",
    "typescript": "^5.3.3",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "playwright": "^1.55.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.10"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "vite --port 3000",
    "type-check": "tsc --noEmit",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:vehicle-counts": "playwright test vehicle-count-comparison",
    "test:interactive": "playwright test interactive-map-controls",
    "mcp:start": "node mcp-playwright-server.js",
    "mcp:test": "node mcp-client-example.js",
    "mcp:test:interactive": "node mcp-client-example.js interactive",
    "mcp:test:monitoring": "node mcp-client-example.js monitoring",
    "mcp:tools": "node mcp-client-example.js tools"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "RTD",
    "Denver",
    "Transit",
    "GTFS-RT",
    "Real-time",
    "Maps",
    "React",
    "TypeScript",
    "OpenStreetMap",
    "Leaflet"
  ],
  "author": "TransitStream Team",
  "license": "MIT"
}
